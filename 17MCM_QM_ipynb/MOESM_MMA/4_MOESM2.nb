    RowBox[{"Function", "[", 
     RowBox[{"\[Kappa]", ",", 
      RowBox[{"Evaluate", "[", "\[IndentingNewLine]", 
       RowBox[{
        FractionBox[
         RowBox[{"Sinc", "[", 
          FractionBox["\[Kappa]", 
