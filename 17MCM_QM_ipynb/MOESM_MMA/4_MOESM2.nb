Cell[TextData[{
 "The Wigner quasi-probability distribution of a density operator ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Rho]", "^"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "68559dbc-ea8e-449b-805b-1168ba122681"],
 " is"
}], "Text",
 CellChangeTimes->{{3.735636194316833*^9, 3.735636214808178*^9}, {
  3.735636299584506*^9, 
  3.735636299952598*^9}},ExpressionUUID->"25e92884-d9f6-430f-ab35-\
29ce26b0f719"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"W", "(", 
    RowBox[{"x", ",", "k"}], ")"}], "=", 
   RowBox[{
    FractionBox["1", "\[Pi]"], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      RowBox[{"\[DifferentialD]", "y"}], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"x", "-", 
        RowBox[{"y", 
         RowBox[{"\[LeftBracketingBar]", 
          OverscriptBox["\[Rho]", "^"], "\[RightBracketingBar]"}], "x"}], "+",
         "y"}], "\[RightAngleBracket]"}], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"2", "\[ImaginaryI]", " ", "k", " ", "y"}]]}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.735636302647935*^9, 
  3.7356363986072083`*^9}},ExpressionUUID->"06de369f-969f-4e21-a890-\
70587dcfc60b"],

