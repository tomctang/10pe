 RowBox[{
  RowBox[{
   RowBox[{"a", "[", 
    RowBox[{"-", "\[Infinity]"}], "]"}], "=", 
   FractionBox["\[Pi]", "2"]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", "0", "]"}], "=", "\[Pi]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", "\[Infinity]", "]"}], "=", 
   FractionBox[
    RowBox[{"3", "\[Pi]"}], "2"]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"a", "[", 
   RowBox[{"\[Kappa]_", "?", "NumericQ"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"a", "[", "\[Kappa]", "]"}], "=", 
   RowBox[{"u", "/.", 
