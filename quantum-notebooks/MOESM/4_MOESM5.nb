     RowBox[{
      RowBox[{
       RowBox[{"u", " ", 
        RowBox[{"Tan", "[", "u", "]"}]}], "\[Equal]", 
       FractionBox["\[Kappa]", "2"]}], ",", 
      RowBox[{"{", 
       RowBox[{"u", ",", 
        RowBox[{"\[Pi]", "+", 
         RowBox[{"ArcTan", "[", 
          FractionBox["\[Kappa]", 
           RowBox[{"2", "\[Pi]"}]], "]"}]}]}], "}"}]}], "]"}]}]}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"b", "[", 
