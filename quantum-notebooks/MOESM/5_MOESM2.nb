     RowBox[{"\[Psi]_", "?", "VectorQ"}], ",", 
     RowBox[{"d_Integer", "/;", 
      RowBox[{"d", "\[GreaterEqual]", "1"}]}]}], "]"}], "/;", 
   RowBox[{"Divisible", "[", 
    RowBox[{
     RowBox[{"Length", "[", "\[Psi]", "]"}], ",", "d"}], "]"}]}], ":=", 
  RowBox[{"rdm", "[", 
   RowBox[{"\[Psi]", ",", 
